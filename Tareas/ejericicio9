nota=float(input("Ingrese su calificacion:"))

if nota>=9 and nota <= 10:
    print(f"{nota:} A")
elif nota>=8 and nota<9:
    print(f"{nota:} B")
elif nota>=7 and nota<8:
    print(f"{nota:} C")
elif nota>=6 and nota<7:
    print(f"{nota:} D")
elif nota>=0 and nota<6:
    print(f"{nota:} F")
else:
    print("Calificacion desconocida") 

